#!/bin/sh
#
# Copyright (C) 2013-2015 OpenWrt.org
#

. /lib/functions/uci-defaults.sh

board_config_update

case "$(board_name)" in
"friendlyelec,nanopi-r2" \
|"friendlyelec,nanopi-r2s")
    ucidef_set_interfaces_lan_wan "eth1" "eth0"
	;;
*)
	ucidef_set_interface_wan 'eth0'
	;;
esac

board_config_flush

exit 0
